{
   "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
   },
   "env": {
      "browser": true,
      "node": true
   },
   "extends": [
      "plugin:vue/recommended",
      "prettier"
   ],
   "plugins": [],
   "rules": {
      "vue/max-attributes-per-line": "off",
      "vue/singleline-html-element-content-newline": "off",
      "indent": "off",
      "vue/no-parsing-error": [2, { "x-invalid-end-tag": false }]
   }
}